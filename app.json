{
  "expo": {
    "name": "996-scanner",
    "slug": "996-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "996scanner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Приложение использует ваше местоположение для определения ближайших заказов.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Приложение использует ваше местоположение для определения ближайших заказов."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAJFy-nMsR_hHhhiMNXYq_-mLjpWlYWbBc"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAJFy-nMsR_hHhhiMNXYq_-mLjpWlYWbBc"
        }
      },
      "package": "com.amanbek22.x996scanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Приложение использует ваше местоположение для определения ближайших заказов и улучшения качества обслуживания.",
          "locationWhenInUsePermission": "Приложение использует ваше местоположение для определения ближайших заказов.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false,
          "isAndroidForegroundServiceEnabled": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
